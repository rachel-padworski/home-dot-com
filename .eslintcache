            company, \n                        img, \n                        info, \n                        price, \n                        title, \n                        inCart\n                    } = value.detailProduct;\n                    return (\n                        <div className=\"container py-5\">\n                            {/* title */}\n                            <div className=\"row\">\n                                <div className=\"col-10 mx-auto text-center text-slanted text-blue my-5\">\n                                    <h1>{console.log(title)}</h1>\n                                </div>\n                            </div>\n                             {/* end title */}\n                             {/* product info */}\n                             <div className=\"row\">\n                                 <div className=\"col-10 mx-auto col-md-6 my-3 text-capitalize\">\n                                    <img src={img} className=\"img-fluid\" alt=\"product\"/>\n                                 </div>\n\n                                 {/* product text */}\n                                 <div className=\"col-10 mx-auto col-md-6 my-3 text-capitalize\">\n                                     <h2>model : {title}</h2>\n                                     <h4 className=\"text-title text-uppercase text-muted mt-3 mb-2\">\n                                         made by : <span className=\"text-uppercase\">\n                                             {company}\n                                         </span>\n                                     </h4>\n                                     <h4 className=\"text-blue\">\n                                         <strong>\n                                             price : <span>$</span>\n                                             {price}\n                                         </strong>\n                                     </h4>\n                                     <p className=\"text-capitalize font-weight-bold mt-3 mb-0\">\n                                         some info about product:\n                                     </p>\n                                     <p className=\"text-muted lead\">{info}</p>\n                                 </div>\n                                 {/* buttons */}\n                                 <div>\n                                    <Link to='/'>\n                                        <ButtonContainer>\n                                            back to products\n                                        </ButtonContainer>\n                                    </Link>\n                                    <ButtonContainer \n                                    disabled={inCart ? true : false}\n                                    onClick={()=>{\n                                        value.addToCart(id)\n                                    }}\n                                    >\n                                        {inCart ? 'inCart' : 'add to cart'}\n                                    </ButtonContainer>\n                                 </div>\n                             </div>\n                             {/* end product info */}\n                        </div>\n                    )\n                }}\n            </ProductConsumer>\n        )\n    }\n}\n","/Users/rachelpadworski/dev/projects/home-dot-com/src/components/Cart.js",[],["58","59"],"/Users/rachelpadworski/dev/projects/home-dot-com/src/components/Product.js",["60","61"],"import React, { Component } from 'react';\nimport styled from 'styled-components';\nimport { Link } from 'react-router-dom';\nimport { ProductConsumer } from '../context';\nimport PropTypes from 'prop-types';\n\nexport default class Product extends Component {\n    render() {\n\n        const { id, title, img, price, inCart } = this.props.product;\n        \n        return (\n            <ProductWrapper className=\"col-9 mx-auto col-md-6 col-lg-3 my-3\">\n                <div className=\"card\">\n                    <div className=\"img-container p-5\" onClick={() => console.log('you clicked me on the img container')}>\n                        <Link to=\"/details\">\n                            <img src={img} alt=\"product\" className=\"card-img-top\"></img>\n                        </Link>\n                        <button \n                            className=\"cart-btn\" \n                            disabled={inCart ? true : false} \n                            onClick={() => console.log('added to cart')}>\n                            {inCart ? \n                                (<p className=\"text-capitalize mb-0\" disabled>in Cart</p>)\n                                :\n                                (<i className=\"fas fa-cart-plus\"/>)\n                            }\n                        </button>\n                    </div>\n                    {/* card footer */}\n                    <div className=\"card-footer d-flex justify-content-between\">\n                        <p className=\"align-self-center mb-0\">\n                            {title}\n                        </p>\n                        <h5 className=\"text-blue font-italic mb-0\">\n                            <span className=\"mr-1\">${price}</span>\n                        </h5>\n                    </div>\n                </div>\n            </ProductWrapper>\n        )\n    }\n}\n\n// propTypes allows us to check the props to make sure we have the correct prop type. \nProduct.propTypes = {\n    product: PropTypes.shape({\n        id: PropTypes.number,\n        img: PropTypes.string,\n        title: PropTypes.string,\n        price: PropTypes.number,\n        inCart: PropTypes.bool\n    }).isRequired\n}\n\nconst ProductWrapper = styled.div`\n.card {\n    border-color:transparent;\n    transition: all 1s linear;\n}\n.card-footer {\n    background: transparent;\n    border-top: transparent;\n    transition: all 1s linear;\n}\n&:hover {\n    .card {\n        border: 0.04rem solid rgba(0,0,0,0.2);\n        box-shadow: 2px 2px 5px 0px rgba(0,0,0,0.2);\n    }\n    .card-footer {\n        background: rgba(247,247,247);\n    }\n}\n.img-container {\n    position: relative;\n    overflow: hidden;\n}\n.card-img-top {\n    transition: all .5s linear;\n}\n.img-container:hover .card-img-top {\n    transform: scale(1.1);\n}\n.cart-btn {\n    position: absolute;\n    bottom: 0;\n    right: 0;\n    padding: 0.2rem 0.4rem;\n    background: var(--lightBlue);\n    border: none;\n    color: var(--mainWhite);\n    font-size: 1.4rem;\n    border-radius: 0.5rem 0 0 0;\n    transform: translate(100%, 100%);\n    transition: all .5s linear;\n}\n.img-container:hover .cart-btn {\n    transform: translate(0, 0);\n}\n.cart-btn:hover {\n    color: var(--mainBlue);\n    cursor: pointer;\n}\n\n`","/Users/rachelpadworski/dev/projects/home-dot-com/src/components/Default.js",[],"/Users/rachelpadworski/dev/projects/home-dot-com/src/components/ProductList.js",[],["62","63"],"/Users/rachelpadworski/dev/projects/home-dot-com/src/components/Button.js",[],"/Users/rachelpadworski/dev/projects/home-dot-com/src/components/Title.js",[],"/Users/rachelpadworski/dev/projects/home-dot-com/src/context.js",[],"/Users/rachelpadworski/dev/projects/home-dot-com/src/data.js",[],{"ruleId":"64","severity":1,"message":"65","line":5,"column":10,"nodeType":"66","messageId":"67","endLine":5,"endColumn":18},{"ruleId":"64","severity":1,"message":"68","line":12,"column":5,"nodeType":"66","messageId":"67","endLine":12,"endColumn":10},{"ruleId":"64","severity":1,"message":"69","line":4,"column":8,"nodeType":"66","messageId":"67","endLine":4,"endColumn":12},{"ruleId":"64","severity":1,"message":"70","line":5,"column":10,"nodeType":"66","messageId":"67","endLine":5,"endColumn":16},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"64","severity":1,"message":"75","line":4,"column":10,"nodeType":"66","messageId":"67","endLine":4,"endC